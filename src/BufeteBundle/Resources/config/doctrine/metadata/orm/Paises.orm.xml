<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Paises" table="paises">
    <id name="idPais" type="integer" column="Id_Pais">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="pais" type="string" column="Pais" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estadoPais" type="boolean" column="Estado_Pais" nullable="false"/>
  </entity>
</doctrine-mapping>
