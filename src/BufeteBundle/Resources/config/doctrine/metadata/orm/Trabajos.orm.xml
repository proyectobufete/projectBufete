<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Trabajos" table="trabajos">
    <id name="idTrabajo" type="integer" column="Id_Trabajo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="trabajo" type="string" column="Trabajo" length="75" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estadoTrabajo" type="boolean" column="Estado_Trabajo" nullable="false"/>
  </entity>
</doctrine-mapping>
