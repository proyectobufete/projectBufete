<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Tribunales" table="tribunales">
    <id name="idTribunal" type="integer" column="Id_Tribunal">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tribunal" type="string" column="Tribunal" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estadoTribunal" type="boolean" column="Estado_Tribunal" nullable="false"/>
  </entity>
</doctrine-mapping>
