        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('demandantes_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('demandantes_edit', { 'idDemandante': demandante.idDemandante }) }}">Edit</a>
