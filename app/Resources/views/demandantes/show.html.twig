{% extends 'base.html.twig' %}

{% block body %}
    <h1>Demandante</h1>

    <table>
        <tbody>
            <tr>
                <th>Iddemandante</th>
                <td>{{ demandante.idDemandante }}</td>
            </tr>
            <tr>
                <th>Nombredemandante</th>
                <td>{{ demandante.nombreDemandante }}</td>
            </tr>
            <tr>
                <th>Edaddemandante</th>
                <td>{{ demandante.edadDemandante }}</td>
            </tr>
            <tr>
