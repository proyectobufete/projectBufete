{% extends 'base.html.twig' %}

{% block body %}
    <h1>Tribunale</h1>

    <table>
        <tbody>
            <tr>
                <th>Idtribunal</th>
                <td>{{ tribunale.idTribunal }}</td>
            </tr>
            <tr>
                <th>Tribunal</th>
                <td>{{ tribunale.tribunal }}</td>
            </tr>
            <tr>
                <th>Estadotribunal</th>
                <td>{% if tribunale.estadoTribunal %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('tribunales_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('tribunales_edit', { 'idTribunal': tribunale.idTribunal }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
